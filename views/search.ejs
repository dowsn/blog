<div class="author">
  <h1 class="author__heading">Search Results</h1>
</div>

<section class="articles">
  <ul class="articles-ul">

    <% data.forEach(p => { %>
    <li>
      <a href="/post/<%= p._id %>">
        <span><%= p.title %></span>
        <span class="article-list__date"><%= p.createdAt.toDateString() %></span>
       </a>
    </li>
    <% }) %>


  </ul>
</section>